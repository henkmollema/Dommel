<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Simple CRUD operations for Dapper. Fork including pagination and support for NO SYNC</Description>
    <Copyright></Copyright>
    <AssemblyTitle>Dommel</AssemblyTitle>
    <VersionPrefix>1.11.0</VersionPrefix>
    <Authors>Henk Mollema, Felipe Silva Pereira</Authors>
    <TargetFrameworks>net451;netstandard1.3;netstandard2.0</TargetFrameworks>
    <TargetFrameworks Condition=" '$(OS)' != 'Windows_NT' ">netstandard1.3;netstandard2.0</TargetFrameworks>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageTags>dommel;crud;dapper;database;orm</PackageTags>
    <PackageProjectUrl>https://github.com/dotzlabs/Dommel</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/dotzlabs/Dommel/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/fspcons/Dommel</RepositoryUrl>
    <PackageId>Dommel.Pag</PackageId>
    <Version>1.2.5</Version>
    <Company>Henk Mollema, Felipe Silva Pereira</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Dapper" Version="1.50.4" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.4.0" />
  </ItemGroup>
</Project>
